<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <menuitem id="project_idea_menuitem" name="Idea Summary"
              parent="project.menu_project_config" />

    <record model="ir.ui.view" id="project_idea_line_viability_page_form_view">
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project" />
        <field name="arch" type="xml">
            <page name="settings" position="before">
                <page string="Viability">
                    <group string="Viability Table">
                        <field name="viability_page" nolabel="1"/>
                    </group>
                </page>
            </page>
        </field>
    </record>
</odoo>
