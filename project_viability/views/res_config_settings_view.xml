<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="project_config_settings_form_view" model="ir.ui.view">
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="project.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('app_settings_block')]" position="inside">
                <h2>Project Viability</h2>
                <div class="row mt16 o_settings_container" title="viability">
                    <div class="col-xs-12 col-md-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <label for="viability_templ_id"/>
                            <span class="fa fa-lg fa-building-o" title="Values set here are company-specific." groups="base.group_multi_company"/>
                            <div class="row">
                                <div class="text-muted col-md-8">
                                    viability_templ_id
                                </div>
                            </div>
                            <div class="content-group">
                                <div class="mt16">
                                    <field name="viability_templ_id" class="o_light_label" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-6 o_setting_box">
                        <div class="o_setting_left_pane">
                            <field name="enable_viability_templ"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="enable_viability_templ"/>
                            <div class="text-muted">
                                enable_viability_templ
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>