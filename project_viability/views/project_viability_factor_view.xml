<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="project_viability_factor_tree_view" model="ir.ui.view">
        <field name="model">project.viability.factor</field>
        <field name="arch" type="xml">
            <tree>
                <field name="code" />
                <field name="name" />
                <field name="categ_id" />
            </tree>
        </field>
    </record>

    <record id="project_viability_factor_form_view" model="ir.ui.view">
        <field name="model">project.viability.factor</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="code" />
                        <field name="name" />
                        <field name="categ_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="project_viability_factor_action" model="ir.actions.act_window">
        <field name="name">Viability Factor</field>
        <field name="res_model">project.viability.factor</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{
        }</field>
    </record>
</odoo>
