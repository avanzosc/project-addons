<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="project_project_resume_view_form" model="ir.ui.view">
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project" />
        <field name="arch" type="xml">
            <notebook position="inside">
                <page name="cost_resume" string="Project Costs">
                    <group name="resume" string="Cost Resume">
                        <field name="resume_ids" nolabel="1"/>
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
